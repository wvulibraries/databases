  <body>
    <!-- Google Tag Manager (noscript) -->
<noscript><iframe src="https://www.googletagmanager.com/ns.html?id=GTM-K523XTD"
height="0" width="0" style="display:none;visibility:hidden"></iframe></noscript>
<!-- End Google Tag Manager (noscript) -->
    <!-- Skip to Navigation -->
    <header>
      <a class="skip-nav" href="#main"> Skip to Main Content </a>
      <%= render 'header/public' %>
    </header>

    <!-- Navigation -->
    <%= render 'public/partials/navigation' %>

    <!-- Templated Content Region -->
    <main id="main" class="wrapper">
      <div class="container">
        <!-- Flash -->
        <%= render "utilities/alerts" %>

        <!-- Yield -->
        <div class="main-content">
          <%= yield %>
        </div>
      </div>
    </main>

    <aside>
      <!-- Help -->
      <%= render 'public/partials/help' %>
      <!-- Mobile Sticky Navigation -->
      <%= render 'public/partials/mobile_navigation' %>
    </aside>

    <!-- Footers -->
    <%= render 'public/partials/footer' %>

    <!-- JavaScripts -->
    <%= yield :js %>

    <!-- Browser Update -->
    <%= render 'shared/browser_update' %>

    <!-- Ask a Librarian JS -->
    <%= render 'public/partials/ask_a_librarian' %>
  </body>
</html>
