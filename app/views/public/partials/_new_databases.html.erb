  <section class="new_database" aria-label="New Databases"> 
    <h2> New </h2>
    <% @new_databases.each do |db| %>
      <div class="database-results"> 
        <%= DatabaseDecorator.new(db).display_title %>
        <p> <%= raw db.description.truncate(150) %> </p>
      </div>
    <% end %>
  </section>